{
  "name": "promptfoo",
  "description": "LLM eval & testing toolkit",
  "author": "Ian Webster",
  "version": "0.17.2",
  "license": "MIT",
  "type": "commonjs",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "types": "dist/src/index.d.ts",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=12"
  },
  "bin": {
    "promptfoo": "dist/src/main.js"
  },
  "scripts": {
    "local": "ts-node --esm src/main.ts",
    "install:client": "cd src/web/client && npm install",
    "build:clean": "rm -rf dist",
    "build:client": "cd src/web/client && npm run build && cp -r dist/ ../../../dist/src/web/client",
    "build:watch": "tsc --watch",
    "build": "tsc && cp src/*.html dist/src && npm run build:client && chmod +x dist/src/main.js",
    "prepare": "npm run install:client && npm run build:clean && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier -w ."
  },
  "devDependencies": {
    "@types/async": "^3.2.20",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-fs-hash": "^0.0.1",
    "@types/cli-progress": "^3.11.0",
    "@types/cors": "^2.8.13",
    "@types/debounce": "^1.2.1",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^20.4.0",
    "@types/node-fetch": "^2.6.4",
    "@types/nunjucks": "^3.2.2",
    "@types/opener": "^1.4.0",
    "@types/semver": "^7.5.0",
    "babel-jest": "^29.5.0",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "prettier": "^2.8.8",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "async": "^3.2.4",
    "cache-manager": "^4.1.0",
    "cache-manager-fs-hash": "^1.0.0",
    "chalk": "^4.1.2",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.3",
    "commander": "^10.0.1",
    "cors": "^2.8.5",
    "csv-parse": "^5.3.8",
    "csv-stringify": "^6.3.2",
    "debounce": "^1.2.1",
    "express": "^4.18.2",
    "glob": "^10.2.6",
    "js-yaml": "^4.1.0",
    "node-fetch": "^2.6.7",
    "nunjucks": "^3.2.4",
    "opener": "^1.5.2",
    "package.json": "^2.0.1",
    "rouge": "^1.0.3",
    "semver": "^7.5.3",
    "socket.io": "^4.6.1",
    "tiny-invariant": "^1.3.1",
    "winston": "^3.8.2"
  }
}
