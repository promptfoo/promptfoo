{
  "name": "promptfoo",
  "description": "LLM eval & testing toolkit",
  "author": "Ian Webster",
  "version": "0.61.0",
  "license": "MIT",
  "type": "commonjs",
  "repository": "promptfoo/promptfoo",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "workspaces": [
    "src/web/nextui",
    "site"
  ],
  "types": "dist/src/index.d.ts",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "promptfoo": "dist/src/main.js"
  },
  "scripts": {
    "bin": "dist/src/main.js",
    "tsc": "tsc",
    "local": "ts-node --cwdMode --transpileOnly src/main.ts",
    "local:web": "cd src/web/nextui && npm run dev",
    "install:nextui": "cd src/web/nextui && npm install",
    "build:clean": "rm -rf dist",
    "build:nextui": "cd src/web/nextui && npm run build && { [ -z \"$NEXT_PUBLIC_PROMPTFOO_BUILD_STANDALONE_SERVER\" ] && cp -r out/ ../../../dist/src/web/nextui; } || true",
    "build:watch": "tsc --watch",
    "build": "tsc && cp src/*.html dist/src && cp src/python/wrapper.py dist/src/python && cp -r drizzle/ dist/ && npm run build:nextui && chmod +x dist/src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier -w .",
    "db:migrate": "npx tsx src/migrate.ts",
    "db:generate": "npx drizzle-kit generate:sqlite",
    "audit:fix": "npm audit fix && cd src/web/nextui && npm audit fix && cd ../../site && npm audit fix"
  },
  "peerDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.458.0",
    "@azure/identity": "^4.0.0",
    "@azure/openai-assistants": "^1.0.0-beta.5",
    "@ibm-generative-ai/node-sdk": "^2.0.6",
    "google-auth-library": "^9.10.0",
    "googleapis": "^137.1.0",
    "langfuse": "^3.11.2"
  },
  "devDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.587.0",
    "@azure/identity": "^4.2.0",
    "@types/async": "^3.2.24",
    "@types/better-sqlite3": "^7.6.10",
    "@types/cache-manager-fs-hash": "^0.0.4",
    "@types/cli-progress": "^3.11.5",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/glob": "^8.1.0",
    "@types/jest": "^29.5.12",
    "@types/js-yaml": "^4.0.9",
    "@types/nunjucks": "^3.2.6",
    "@types/opener": "^1.4.3",
    "@types/semver": "^7.5.8",
    "@types/uuid": "^9.0.8",
    "babel-jest": "^29.7.0",
    "cloudflare": "^3.2.0",
    "check-dependency-version-consistency": "^4.1.0",
    "drizzle-kit": "^0.21.4",
    "jest": "^29.7.0",
    "jest-watch-typeahead": "^2.2.2",
    "next": "13.4.13",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.22.0",
    "@apidevtools/json-schema-ref-parser": "^11.6.2",
    "ajv": "^8.14.0",
    "ajv-formats": "^3.0.1",
    "async": "^3.2.5",
    "better-sqlite3": "^10.1.0",
    "cache-manager": "^5.5.3",
    "cache-manager-fs-hash": "^2.0.0",
    "chalk": "^5.3.0",
    "chokidar": "^3.6.0",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.6",
    "csv-stringify": "^6.5.0",
    "debounce": "^2.1.0",
    "dedent": "^1.5.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.30.10",
    "express": "^4.19.2",
    "fast-deep-equal": "^3.1.3",
    "fastest-levenshtein": "^1.0.16",
    "glob": "^10.4.1",
    "js-yaml": "^4.1.0",
    "mathjs": "^12.4.2",
    "node-fetch": "^3.3.2",
    "nunjucks": "^3.2.4",
    "openai": "^4.47.2",
    "opener": "^1.5.2",
    "proxy-agent": "^6.4.0",
    "python-shell": "^5.0.0",
    "replicate": "^0.30.1",
    "rouge": "git+https://github.com/kenlimmj/rouge.git#f35111b599aca55f1d4dc1d4a3d15e28e7f7c55f",
    "semver": "^7.6.2",
    "socket.io": "^4.7.5",
    "tiny-invariant": "^1.3.3",
    "uuid": "^9.0.1",
    "winston": "^3.13.0"
  }
}
