{
  "name": "promptfoo",
  "description": "LLM eval & testing toolkit",
  "author": "Ian Webster",
  "version": "0.62.1",
  "license": "MIT",
  "type": "commonjs",
  "repository": "promptfoo/promptfoo",
  "main": "dist/src/index.js",
  "exports": {
    ".": {
      "import": "./dist/src/index.js",
      "require": "./dist/src/index.js"
    }
  },
  "workspaces": [
    "src/web/nextui",
    "site"
  ],
  "types": "dist/src/index.d.ts",
  "typings": "dist/src/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=18"
  },
  "bin": {
    "promptfoo": "dist/src/main.js"
  },
  "scripts": {
    "bin": "dist/src/main.js",
    "tsc": "tsc",
    "local": "ts-node --cwdMode --transpileOnly src/main.ts",
    "local:web": "npm run dev --prefix src/web/nextui",
    "install:nextui": "npm install --prefix src/web/nextui",
    "build:clean": "shx rm -rf dist",
    "build:nextui": "shx mkdir -p dist/src/web/nextui && npm run build --prefix src/web/nextui && node -e \"if (!process.env.NEXT_PUBLIC_PROMPTFOO_BUILD_STANDALONE_SERVER) require('child_process').execSync('shx cp -r src/web/nextui/out dist/src/web/nextui', {stdio: 'inherit'})\"",
    "build:watch": "tsc --watch",
    "build": "tsc && shx cp src/*.html dist/src && shx cp src/python/wrapper.py dist/src/python && shx rm -rf dist/drizzle && shx cp -r drizzle dist/drizzle && npm run build:nextui && shx chmod +x dist/src/main.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "format": "prettier -w .",
    "format:check": "prettier --check .",
    "db:migrate": "npx tsx src/migrate.ts",
    "db:generate": "npx drizzle-kit generate:sqlite",
    "audit:fix": "npm audit fix && npm audit fix --prefix src/web/nextui && npm audit fix --prefix site",
    "prepublishOnly": "npm run build:clean && npm run build"
  },
  "peerDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.458.0",
    "@azure/identity": "^4.0.0",
    "@azure/openai-assistants": "^1.0.0-beta.5",
    "@ibm-generative-ai/node-sdk": "^2.0.6",
    "google-auth-library": "^9.7.0",
    "googleapis": "^134.0.0",
    "langfuse": "^3.7.0"
  },
  "devDependencies": {
    "@aws-sdk/client-bedrock-runtime": "^3.458.0",
    "@azure/identity": "^4.0.0",
    "@types/async": "^3.2.20",
    "@types/better-sqlite3": "^7.6.10",
    "@types/cache-manager": "^4.0.2",
    "@types/cache-manager-fs-hash": "^0.0.1",
    "@types/cli-progress": "^3.11.0",
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.13",
    "@types/debounce": "^1.2.1",
    "@types/express": "^4.17.17",
    "@types/glob": "^8.1.0",
    "@types/inquirer": "^8.2.6",
    "@types/jest": "^29.5.1",
    "@types/js-yaml": "^4.0.5",
    "@types/node-fetch": "^2.6.4",
    "@types/nunjucks": "^3.2.2",
    "@types/opener": "^1.4.0",
    "@types/semver": "^7.5.0",
    "@types/uuid": "^9.0.2",
    "babel-jest": "^29.5.0",
    "check-dependency-version-consistency": "^4.1.0",
    "cloudflare": "^3.2.0",
    "drizzle-kit": "^0.20.13",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "next": "13.4.13",
    "prettier": "^2.8.8",
    "shx": "^0.3.4",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.1",
    "@apidevtools/json-schema-ref-parser": "^10.1.0",
    "ajv": "^8.14.0",
    "ajv-formats": "^2.1.1",
    "async": "^3.2.4",
    "better-sqlite3": "^10.1.0",
    "cache-manager": "^4.1.0",
    "cache-manager-fs-hash": "^1.0.0",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "cli-progress": "^3.12.0",
    "cli-table3": "^0.6.3",
    "commander": "^10.0.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "csv-parse": "^5.5.3",
    "csv-stringify": "^6.3.2",
    "debounce": "^1.2.1",
    "dedent": "^1.5.3",
    "dotenv": "^16.4.5",
    "drizzle-orm": "^0.29.3",
    "express": "^4.18.2",
    "fast-deep-equal": "^3.1.3",
    "fastest-levenshtein": "^1.0.16",
    "glob": "^10.2.6",
    "inquirer": "^8.2.6",
    "js-yaml": "^4.1.0",
    "mathjs": "^12.4.1",
    "node-fetch": "^2.6.7",
    "nunjucks": "^3.2.4",
    "openai": "^4.38.5",
    "opener": "^1.5.2",
    "proxy-agent": "^6.3.1",
    "python-shell": "^5.0.0",
    "replicate": "^0.27.1",
    "rouge": "git+https://github.com/kenlimmj/rouge.git#f35111b599aca55f1d4dc1d4a3d15e28e7f7c55f",
    "semver": "^7.5.3",
    "socket.io": "^4.7.2",
    "tiny-invariant": "^1.3.1",
    "uuid": "^9.0.0",
    "winston": "^3.8.2"
  }
}
