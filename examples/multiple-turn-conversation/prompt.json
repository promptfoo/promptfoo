[
  {
    "role": "system",
    "content": "{{ system_message }}"
  },
  {% for completion in _conversation %}
    {
    "role": "user",
    "content": "{{ completion.prompt[completion.prompt.length - 1].content | safe }}"
  },
  {
    "role": "assistant",
    "content": "{{ completion.output | dump }}"
  },
  {% endfor %}
  {
    "role": "user",
    "content": "{{ question | safe }}"
  }
]