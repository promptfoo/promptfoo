{
  "compilerOptions": {
    "declaration": true,
    "declarationMap": true,
    "esModuleInterop": true,
    "target": "es2020",
    "module": "CommonJS",
    "moduleResolution": "node",
    "outDir": "dist/server",
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "resolveJsonModule": true
  },
  "include": ["src/web/execServer.ts", "typings/**/*"],
  "exclude": ["node_modules", "dist"]
}
