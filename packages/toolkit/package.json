{
  "name": "promptfoo-toolkit",
  "version": "1.0.0",
  "description": "A React component library for promptfoo",
  "private": true,
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "style": "dist/style.css",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "style": "./dist/style.css",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "type-check": "tsc --noEmit",
    "test": "vitest",
    "test:ci": "vitest run",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "react",
    "components",
    "promptfoo",
    "toolkit",
    "ui"
  ],
  "author": "promptfoo team",
  "license": "MIT",
  "peerDependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@mui/icons-material": "^7.3.2",
    "@mui/material": "^7.3.2",
    "@mui/system": "^7.3.2",
    "@mui/x-charts": "^7.29.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.64.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "typescript": "^5.9.2",
    "vite": "^4.4.0",
    "vite-plugin-dts": "^3.5.0",
    "vitest": "^3.2.4"
  }
}
